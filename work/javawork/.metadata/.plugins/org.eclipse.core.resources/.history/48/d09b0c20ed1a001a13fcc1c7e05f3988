package oop.chap06;
//Person클래스를 사용하는 클래스
public class PersonTest {
	public static void main(String[] args) {
		Person p1 = new Person();
		p1.name = "장동건";
		p1.addr = "서울시";
		p1.age = 45;
		
		System.out.println("성명:"+p1.name);
		System.out.println("주소:"+p1.addr);
		System.out.println("나이:"+p1.age);
	}

}
