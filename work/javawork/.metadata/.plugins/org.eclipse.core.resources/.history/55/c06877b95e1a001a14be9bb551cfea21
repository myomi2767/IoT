package chap05;

public class ArrayEx6_book {
	public static void main(String[] args) {
		int score[] = {79,88,91,33,100,55,95};
		
		int max = score[0];
		int min = score[0];
		
		for(int i=0;i<score.length;i++) {
			if(score[i]>max) {
				max = score[i];
			}else{
				min = score[i];
			}
		}
	}

}
